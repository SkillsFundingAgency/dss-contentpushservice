      "type": "string"
    },
    "cosmosDbDatabaseId": {
      "type": "string"
    },
    "cosmosDbEndpoint": {
      "type": "string"
    },
    "cosmosDbKey": {
      "type": "string"
    },
    "environmentName": {
      "type": "string"
    },
    "functionAppNames": {
      "type": "array"
    },
    "serviceBusConnectionString": {
      "type": "string"
    },
    "sharedAppServicePlanName": {
      "type": "string"
    },
    "sharedAppServicePlanResourceGroup": {
      "type": "string"
    },
    "templateBaseUrl": {
      "type": "string"
