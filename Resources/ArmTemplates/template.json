      "type": "string"
    },
    "northWest.Url": {
      "type": "string"
    },
    "southEast.AppIdUri": {
      "type": "string"
    },
    "southEast.Url": {
      "type": "string"
    },
    "southWestAndOxford.AppIdUri": {
      "type": "string"
    },
    "southWestAndOxford.Url": {
      "type": "string"
